# ==========================
# ATLAS PRODUCTION CONFIG
# ==========================
# This file is used for Railway deployment
# Copy this to .env.production and fill in your values
# Railway will automatically load .env.production in production

# --- Supabase ---
SUPABASE_URL=https://YOUR-PROJECT-ID.supabase.co
SUPABASE_ANON_KEY=your-anon-key-here
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key-here

# --- Paddle (Live Production) ---
VITE_PADDLE_ENVIRONMENT=production
VITE_PADDLE_CLIENT_TOKEN=your-live-client-token
VITE_PADDLE_CORE_PRICE_ID=pri_core_id_here
VITE_PADDLE_STUDIO_PRICE_ID=pri_studio_id_here

# --- Atlas App ---
PORT=3000
NODE_ENV=production

# --- Frontend ---
VITE_API_URL=https://atlas-production-14090287.up.railway.app
VITE_SUPABASE_URL=https://YOUR-PROJECT-ID.supabase.co
VITE_SUPABASE_ANON_KEY=your-anon-key-here

# --- Model Configuration ---
DEFAULT_MODEL=claude-3-haiku-20240307
FREE_TIER_LIMIT=15

# --- Monitoring ---
ENABLE_LOGGING=true
LOG_LEVEL=info

# --- Debug Options ---
VITE_DEBUG_AUTH=0