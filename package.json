{
  "name": "atlas-backend",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "main": "backend/server.mjs",
  "engines": {
    "node": ">=22.12.0",
    "npm": ">=8.0.0"
  },
  "scripts": {
    "start": "node backend/server.mjs",
    "dev": "nodemon backend/server.mjs",
    "dev:backend": "node scripts/kill-port-and-run-backend.js",
    "dev:frontend": "vite",
    "dev:full": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "build": "vite build",
    "preview": "vite preview",
    "postinstall": "echo 'Backend dependencies installed successfully'",
    "health": "curl http://localhost:$PORT/healthz || echo 'Health check failed'",
    "kill-port": "node scripts/kill-port-and-run-backend.js",
    "port-clear": "lsof -ti :8000 | xargs kill -9 2>/dev/null || echo 'Port 8000 is free'"
  },
  "dependencies": {
    "@supabase/auth-ui-react": "^0.4.7",
    "@supabase/auth-ui-shared": "^0.1.8",
    "@supabase/supabase-js": "^2.50.5",
    "compression": "^1.8.0",
    "cors": "^2.8.5",
    "dexie": "^4.2.0",
    "dotenv": "^17.2.0",
    "express": "^4.18.2",
    "express-rate-limit": "^7.5.1",
    "framer-motion": "^12.23.12",
    "helmet": "^8.1.0",
    "lucide-react": "^0.542.0",
    "morgan": "^1.10.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/postcss": "^4.1.12",
    "@types/react": "^19.1.12",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.2",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.2.1",
    "eslint": "^9.34.0",
    "nodemon": "^3.1.10",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.12",
    "terser": "^5.43.1",
    "typescript": "^5.9.2",
    "vite": "^7.1.3"
  }
}
