{
  "name": "atlas-backend",
  "version": "1.0.0",
  "description": "Atlas - Emotionally Intelligent AI Assistant",
  "type": "module",
  "scripts": {
    "dev": "NODE_ENV=development USE_MOCK_MAILER=true vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "type-check": "tsc --noEmit",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "backend": "node backend/server.mjs",
    "backend:dev": "nodemon backend/server.mjs",
    "start": "concurrently \"npm run backend\" \"npm run dev\"",
    "start:dev": "concurrently \"npm run backend:dev\" \"npm run dev\"",
    "redis:start": "redis-server",
    "redis:stop": "redis-cli shutdown",
    "redis:status": "redis-cli ping",
    "cache:stats": "node -e \"import('./src/services/redisCacheService.js').then(m => console.log(m.redisCacheService.getStats()))\"",
    "cache:clear": "node -e \"import('./src/services/redisCacheService.js').then(m => m.redisCacheService.shutdown())\""
  },
