{
  "name": "atlas-backend",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "main": "backend/server.mjs",
  "engines": {
    "node": ">=22.12.0"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "eslint --cache --fix"
    ]
  },
  "scripts": {
    "prepare": "husky",
    "start": "NODE_ENV=development node backend/server.mjs",
    "dev": "NODE_ENV=development USE_MOCK_MAILER=true vite",
    "test:env": "jest backend/__tests__/env.test.js --config='{\"testEnvironment\":\"node\"}'",
    "nuke-db": "node scripts/nuke-db.js",
    "dev:qr": "./scripts/dev-qr.sh",
    "dev:backend": "NODE_ENV=development nodemon backend/server.mjs",
    "dev:frontend": "NODE_ENV=development USE_MOCK_MAILER=true vite",
    "backend": "NODE_ENV=development node backend/server.mjs",
    "backend:restart": "pkill -f 'node backend/server.mjs' || true && sleep 2 && npm run backend",
    "frontend": "NODE_ENV=development USE_MOCK_MAILER=true vite",
    "dev:all": "concurrently \"npm run backend\" \"npm run frontend\"",
    "dev:web": "NODE_ENV=development USE_MOCK_MAILER=true vite --port 5174",
    "dev:native": "expo start",
    "atlas": "./dev.sh",
    "prebuild": "node scripts/checkEnv.mjs",
    "build": "tsc && NODE_ENV=production vite build",
    "preview": "vite preview",
    "check-env": "node scripts/checkEnv.mjs",
    "create-test-user": "ts-node --compiler-options '{\"module\":\"CommonJS\"}' scripts/create-test-user.ts || node scripts/create-test-user.ts",
    "test": "NODE_ENV=test vitest --passWithNoTests --run",
    "test:ui": "NODE_ENV=test vitest --ui",
    "lint": "eslint src/ backend/ --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint src/ backend/ --ext .ts,.tsx,.js,.jsx --fix",
    "format": "prettier --write src/ backend/",
    "test:run": "vitest run",
    "test:e2e": "playwright test",
    "test:fastspring": "ts-node scripts/test-fastspring-webhook.ts",
    "test:tier-updates": "ts-node scripts/test-tier-updates.ts",
    "test:realtime": "ts-node scripts/test-realtime-updates.ts",
    "check:failures": "ts-node scripts/checkFailures.ts",
    "send:test": "node scripts/sendTestEmail.cjs",
    "typecheck": "tsc --noEmit",
    "verify": "npm run lint && npm run typecheck && npm run test",
    "qa": "./scripts/qa-test-suite.sh",
    "monitoring": "./scripts/monitoring-verification.sh",
    "staging": "./scripts/staging-validation.sh",
    "postinstall": "bash scripts/fix-husky.sh || true",
    "health": "curl http://localhost:${PORT:-3000}/api/healthz || echo 'Health check failed'",
    "test:railway": "./test-railway-health.sh",
    "test:compare": "./compare-endpoints.sh",
    "test:local-health": "curl -s http://localhost:3000/healthz | jq . || echo 'Local server not running'",
    "monitor": "node scripts/monitoring/railway-monitor.mjs",
    "monitor:dashboard": "node scripts/monitoring/dashboard.mjs",
    "monitor:setup": "echo 'ðŸ“Š Setting up monitoring...' && mkdir -p logs scripts/monitoring",
    "android": "expo run:android",
    "ios": "expo run:ios"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.63.0",
    "@popperjs/core": "^2.11.8",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@supabase/auth-ui-react": "^0.4.7",
    "@supabase/auth-ui-shared": "^0.1.8",
    "@supabase/supabase-js": "^2.50.5",
    "@tanstack/react-query": "^5.87.1",
    "@types/dexie": "^1.3.32",
    "@types/lodash": "^4.17.20",
    "@types/multer": "^2.0.0",
    "@types/node-fetch": "^2.6.13",
    "@types/react-syntax-highlighter": "^15.5.13",
    "axios": "^1.12.2",
    "base64-arraybuffer": "^1.0.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "compression": "^1.8.0",
    "cors": "^2.8.5",
    "crypto-js": "^4.2.0",
    "dexie": "^4.2.0",
    "dexie-react-hooks": "^4.2.0",
    "dotenv": "^17.2.3",
    "dotenv-safe": "^9.1.0",
    "expo": "^54.0.9",
    "expo-av": "^16.0.7",
    "expo-file-system": "^19.0.14",
    "expo-image-manipulator": "^14.0.7",
    "expo-image-picker": "^17.0.8",
    "express": "^4.18.2",
    "express-rate-limit": "^7.5.1",
    "framer-motion": "^12.23.12",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "katex": "^0.16.22",
    "lodash": "^4.17.21",
    "lucide-react": "^0.542.0",
    "morgan": "^1.10.0",
    "multer": "^2.0.2",
    "node-cron": "^4.2.1",
    "node-fetch": "^3.3.2",
    "nodemailer": "^7.0.6",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-dropzone": "^14.3.8",
    "react-error-boundary": "^6.0.0",
    "react-hot-toast": "^2.6.0",
    "react-katex": "^3.1.0",
    "react-markdown": "^10.1.0",
    "react-native": "0.81.4",
    "react-native-web": "^0.21.0",
    "react-router-dom": "^7.8.2",
    "react-syntax-highlighter": "^15.6.6",
    "rehype-raw": "^7.0.0",
    "remark-gfm": "^4.0.1",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.3.1",
    "uuid": "^11.1.0",
    "zod": "^4.1.8",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.35.0",
    "@playwright/test": "^1.55.0",
    "@sentry/browser": "^10.10.0",
    "@tailwindcss/forms": "^0.5.10",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/crypto-js": "^4.2.2",
    "@types/node": "^24.5.1",
    "@types/react": "^19.1.12",
    "@types/react-dom": "^19.1.9",
    "@types/react-katex": "^3.0.4",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^8.44.0",
    "@typescript-eslint/parser": "^8.44.0",
    "@vitejs/plugin-react": "^5.0.2",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.2.1",
    "eslint": "^9.36.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "fake-indexeddb": "^6.2.2",
    "globals": "^16.4.0",
    "husky": "^9.1.7",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "jsdom": "^25.0.1",
    "lint-staged": "^16.1.6",
    "msw": "^2.11.1",
    "nodemon": "^3.1.10",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "qrcode-terminal": "^0.12.0",
    "tailwindcss": "^3.4.0",
    "terser": "^5.43.1",
    "ts-jest": "^29.4.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2",
    "vite": "^7.1.3",
    "vitest": "^2.1.8",
    "winston": "^3.17.0"
  }
}
